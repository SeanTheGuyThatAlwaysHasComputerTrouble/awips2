/* ------------------------------------------------------------------------- */

int UpdateCoopSponsByRecord (const CoopSpons * newStructPtr, const CoopSpons * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetCoopSponsPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateCoopSpons(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateCoopSpons(const CoopSpons * structPtr)
{
    UpdateCoopSponsByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertCoopSpons(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueCoopSpons(const CoopSpons * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (CoopSponsExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertCoopSpons(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteCoopSponsByRecord(const CoopSpons * structPtr)
{
    char whereClause[BUFSIZ];
    GetCoopSponsPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteCoopSpons(whereClause));
}

/* ------------------------------------------------------------------------- */

bool CoopSponsExists(const CoopSpons * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetCoopSponsPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectCoopSponsCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetCoopSponsPrimaryKeyWhereString (const CoopSpons * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE spons = '%s' ",
            structPtr->spons);
}

/* ------------------------------------------------------------------------- */

void FreeCoopSpons( CoopSpons * structPtr)
{
    CoopSpons* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( CoopSpons * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

