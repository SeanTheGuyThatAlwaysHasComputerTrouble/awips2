/* ------------------------------------------------------------------------- */

int UpdateTelmOwnerByRecord (const TelmOwner * newStructPtr, const TelmOwner * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetTelmOwnerPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateTelmOwner(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateTelmOwner(const TelmOwner * structPtr)
{
    UpdateTelmOwnerByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertTelmOwner(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueTelmOwner(const TelmOwner * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (TelmOwnerExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertTelmOwner(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteTelmOwnerByRecord(const TelmOwner * structPtr)
{
    char whereClause[BUFSIZ];
    GetTelmOwnerPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteTelmOwner(whereClause));
}

/* ------------------------------------------------------------------------- */

bool TelmOwnerExists(const TelmOwner * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetTelmOwnerPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectTelmOwnerCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetTelmOwnerPrimaryKeyWhereString (const TelmOwner * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE owner = '%s' ",
            structPtr->owner);
}

/* ------------------------------------------------------------------------- */

void FreeTelmOwner( TelmOwner * structPtr)
{
    TelmOwner* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( TelmOwner * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

