/* ------------------------------------------------------------------------- */

int UpdateWatSupMethodByRecord (const WatSupMethod * newStructPtr, const WatSupMethod * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetWatSupMethodPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateWatSupMethod(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateWatSupMethod(const WatSupMethod * structPtr)
{
    UpdateWatSupMethodByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertWatSupMethod(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueWatSupMethod(const WatSupMethod * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (WatSupMethodExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertWatSupMethod(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteWatSupMethodByRecord(const WatSupMethod * structPtr)
{
    char whereClause[BUFSIZ];
    GetWatSupMethodPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteWatSupMethod(whereClause));
}

/* ------------------------------------------------------------------------- */

bool WatSupMethodExists(const WatSupMethod * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetWatSupMethodPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectWatSupMethodCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetWatSupMethodPrimaryKeyWhereString (const WatSupMethod * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE watsup_method = '%s' ",
            structPtr->watsup_method);
}

/* ------------------------------------------------------------------------- */

void FreeWatSupMethod( WatSupMethod * structPtr)
{
    WatSupMethod* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( WatSupMethod * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

