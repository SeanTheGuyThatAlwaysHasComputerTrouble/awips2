WatSupMethod * GetWatSupMethod(const char * where)
{

    WatSupMethod * listPtr = NULL;
    WatSupMethod * structPtr = NULL;
    char selectStatement[] = "SELECT * FROM WatSupMethod ";

    int rowCount = 0;
    int first = 1;

    EXEC SQL BEGIN DECLARE SECTION;

