int DeleteHsa(const char *where)
{
    char deleteStatement[] = "DELETE FROM Hsa ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteHsa() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateHsaByRecord (const Hsa * newStructPtr, const Hsa * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetHsaPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateHsa(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateHsa(const Hsa * structPtr)
{
    UpdateHsaByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertHsa(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueHsa(const Hsa * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (HsaExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertHsa(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteHsaByRecord(const Hsa * structPtr)
{
    char whereClause[BUFSIZ];
    GetHsaPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteHsa(whereClause));
}

/* ------------------------------------------------------------------------- */

bool HsaExists(const Hsa * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetHsaPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectHsaCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetHsaPrimaryKeyWhereString (const Hsa * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE hsa = '%s' ",
            structPtr->hsa);
}

/* ------------------------------------------------------------------------- */

void FreeHsa( Hsa * structPtr)
{
    Hsa* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( Hsa * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetHsaDbStatus()
{
    return &dbStatus;
}

/* ------------------------------------------------------------------------- */

void SetHsaErrorLogging(int value)
{
    errorLoggingOn = value;
    return;
}


