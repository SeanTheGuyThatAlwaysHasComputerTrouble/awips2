int DeleteGageMaint(const char *where)
{
    char deleteStatement[] = "DELETE FROM GageMaint ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteGageMaint() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateGageMaintByRecord (const GageMaint * newStructPtr, const GageMaint * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetGageMaintPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateGageMaint(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateGageMaint(const GageMaint * structPtr)
{
    UpdateGageMaintByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertGageMaint(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueGageMaint(const GageMaint * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (GageMaintExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertGageMaint(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteGageMaintByRecord(const GageMaint * structPtr)
{
    char whereClause[BUFSIZ];
    GetGageMaintPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteGageMaint(whereClause));
}

/* ------------------------------------------------------------------------- */

bool GageMaintExists(const GageMaint * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetGageMaintPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectGageMaintCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetGageMaintPrimaryKeyWhereString (const GageMaint * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE maint = '%s' ",
            structPtr->maint);
}

/* ------------------------------------------------------------------------- */

void FreeGageMaint( GageMaint * structPtr)
{
    GageMaint* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( GageMaint * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

