int DeleteDatum(const char *where)
{
    char deleteStatement[] = "DELETE FROM Datum ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteDatum() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateDatumByRecord (const Datum * newStructPtr, const Datum * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetDatumPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateDatum(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateDatum(const Datum * structPtr)
{
    UpdateDatumByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertDatum(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueDatum(const Datum * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (DatumExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertDatum(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteDatumByRecord(const Datum * structPtr)
{
    char whereClause[BUFSIZ];
    GetDatumPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteDatum(whereClause));
}

/* ------------------------------------------------------------------------- */

bool DatumExists(const Datum * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetDatumPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectDatumCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetDatumPrimaryKeyWhereString (const Datum * structPtr, char returnWhereString[] )
{
    char date_buffer0[40];
    sprintf(returnWhereString, "WHERE lid = '%s'  AND ddate = '%s' ",
            structPtr->lid, date_t_to_ansi_date(structPtr->ddate, date_buffer0));
}

/* ------------------------------------------------------------------------- */

void FreeDatum( Datum * structPtr)
{
    Datum* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( Datum * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

