/* ------------------------------------------------------------------------- */

int UpdateFcstTypeByRecord (const FcstType * newStructPtr, const FcstType * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetFcstTypePrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateFcstType(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateFcstType(const FcstType * structPtr)
{
    UpdateFcstTypeByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertFcstType(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueFcstType(const FcstType * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (FcstTypeExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertFcstType(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteFcstTypeByRecord(const FcstType * structPtr)
{
    char whereClause[BUFSIZ];
    GetFcstTypePrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteFcstType(whereClause));
}

/* ------------------------------------------------------------------------- */

bool FcstTypeExists(const FcstType * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetFcstTypePrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectFcstTypeCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetFcstTypePrimaryKeyWhereString (const FcstType * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE fcsttype = '%s' ",
            structPtr->fcsttype);
}

/* ------------------------------------------------------------------------- */

void FreeFcstType( FcstType * structPtr)
{
    FcstType* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( FcstType * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

