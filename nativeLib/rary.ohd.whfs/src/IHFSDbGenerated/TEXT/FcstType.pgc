    strcpy(dbs.fcsttype, structPtr->fcsttype);


    sprintf(queryBuffer, " UPDATE FcstType SET " );
    strcat(queryBuffer, "fcsttype = ? ");

    if ( (where != NULL) && (*where != '\0'))
