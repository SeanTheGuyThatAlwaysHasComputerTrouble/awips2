bool LocExtAgencyExists(const LocExtAgency * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetLocExtAgencyPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectLocExtAgencyCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetLocExtAgencyPrimaryKeyWhereString (const LocExtAgency * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE lid = '%s'  AND agency_code = '%s'  AND office = '%s' ",
            structPtr->lid, structPtr->agency_code, structPtr->office);
}

