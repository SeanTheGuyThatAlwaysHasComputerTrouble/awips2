void FreeLocExtAgency( LocExtAgency * structPtr)
{
    LocExtAgency* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( LocExtAgency * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
