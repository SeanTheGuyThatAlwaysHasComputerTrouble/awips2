int DeleteRfc(const char *where)
{
    char deleteStatement[] = "DELETE FROM Rfc ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteRfc() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateRfcByRecord (const Rfc * newStructPtr, const Rfc * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetRfcPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateRfc(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateRfc(const Rfc * structPtr)
{
    UpdateRfcByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertRfc(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueRfc(const Rfc * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (RfcExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertRfc(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteRfcByRecord(const Rfc * structPtr)
{
    char whereClause[BUFSIZ];
    GetRfcPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteRfc(whereClause));
}

/* ------------------------------------------------------------------------- */

bool RfcExists(const Rfc * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetRfcPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectRfcCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetRfcPrimaryKeyWhereString (const Rfc * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE rfc = '%s' ",
            structPtr->rfc);
}

/* ------------------------------------------------------------------------- */

void FreeRfc( Rfc * structPtr)
{
    Rfc* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( Rfc * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetRfcDbStatus()
{
    return &dbStatus;
}

/* ------------------------------------------------------------------------- */

void SetRfcErrorLogging(int value)
{
    errorLoggingOn = value;
    return;
}


