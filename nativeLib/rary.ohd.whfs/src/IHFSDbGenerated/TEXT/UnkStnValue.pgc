{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetUnkStnValuePrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectUnkStnValueCount(whereString);

    if (rowCount > 0)
