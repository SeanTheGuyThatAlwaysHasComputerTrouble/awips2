void FreeUnkStnValue( UnkStnValue * structPtr)
{
    UnkStnValue* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( UnkStnValue * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

