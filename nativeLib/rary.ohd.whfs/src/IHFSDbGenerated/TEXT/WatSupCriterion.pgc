        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateWatSupCriterionByRecord (const WatSupCriterion * newStructPtr, const WatSupCriterion * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetWatSupCriterionPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateWatSupCriterion(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateWatSupCriterion(const WatSupCriterion * structPtr)
{
    UpdateWatSupCriterionByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertWatSupCriterion(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueWatSupCriterion(const WatSupCriterion * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (WatSupCriterionExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertWatSupCriterion(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteWatSupCriterionByRecord(const WatSupCriterion * structPtr)
{
    char whereClause[BUFSIZ];
    GetWatSupCriterionPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteWatSupCriterion(whereClause));
}

/* ------------------------------------------------------------------------- */

bool WatSupCriterionExists(const WatSupCriterion * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetWatSupCriterionPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectWatSupCriterionCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetWatSupCriterionPrimaryKeyWhereString (const WatSupCriterion * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE watsup_criterion = '%s' ",
            structPtr->watsup_criterion);
}

/* ------------------------------------------------------------------------- */

void FreeWatSupCriterion( WatSupCriterion * structPtr)
{
    WatSupCriterion* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( WatSupCriterion * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

