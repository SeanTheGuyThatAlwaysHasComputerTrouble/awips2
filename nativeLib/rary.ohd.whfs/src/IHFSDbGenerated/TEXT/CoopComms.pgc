/* ------------------------------------------------------------------------- */

int UpdateCoopCommsByRecord (const CoopComms * newStructPtr, const CoopComms * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetCoopCommsPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateCoopComms(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateCoopComms(const CoopComms * structPtr)
{
    UpdateCoopCommsByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertCoopComms(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueCoopComms(const CoopComms * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (CoopCommsExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertCoopComms(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteCoopCommsByRecord(const CoopComms * structPtr)
{
    char whereClause[BUFSIZ];
    GetCoopCommsPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteCoopComms(whereClause));
}

/* ------------------------------------------------------------------------- */

bool CoopCommsExists(const CoopComms * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetCoopCommsPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectCoopCommsCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetCoopCommsPrimaryKeyWhereString (const CoopComms * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE comm = '%s' ",
            structPtr->comm);
}

/* ------------------------------------------------------------------------- */

void FreeCoopComms( CoopComms * structPtr)
{
    CoopComms* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( CoopComms * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

