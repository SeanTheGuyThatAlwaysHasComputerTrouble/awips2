/* ------------------------------------------------------------------------- */

int UpdateServiceTypeByRecord (const ServiceType * newStructPtr, const ServiceType * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetServiceTypePrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateServiceType(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateServiceType(const ServiceType * structPtr)
{
    UpdateServiceTypeByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertServiceType(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueServiceType(const ServiceType * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (ServiceTypeExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertServiceType(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteServiceTypeByRecord(const ServiceType * structPtr)
{
    char whereClause[BUFSIZ];
    GetServiceTypePrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteServiceType(whereClause));
}

/* ------------------------------------------------------------------------- */

bool ServiceTypeExists(const ServiceType * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetServiceTypePrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectServiceTypeCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetServiceTypePrimaryKeyWhereString (const ServiceType * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE service_type = '%s' ",
            structPtr->service_type);
}

/* ------------------------------------------------------------------------- */

void FreeServiceType( ServiceType * structPtr)
{
    ServiceType* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( ServiceType * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

