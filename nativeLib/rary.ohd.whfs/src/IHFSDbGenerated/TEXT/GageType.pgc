/* ------------------------------------------------------------------------- */

int UpdateGageTypeByRecord (const GageType * newStructPtr, const GageType * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetGageTypePrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateGageType(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateGageType(const GageType * structPtr)
{
    UpdateGageTypeByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertGageType(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueGageType(const GageType * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (GageTypeExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertGageType(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteGageTypeByRecord(const GageType * structPtr)
{
    char whereClause[BUFSIZ];
    GetGageTypePrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteGageType(whereClause));
}

/* ------------------------------------------------------------------------- */

bool GageTypeExists(const GageType * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetGageTypePrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectGageTypeCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetGageTypePrimaryKeyWhereString (const GageType * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE type = '%s' ",
            structPtr->type);
}

/* ------------------------------------------------------------------------- */

void FreeGageType( GageType * structPtr)
{
    GageType* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( GageType * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

