int DeleteVTECaction(const char *where)
{
    char deleteStatement[] = "DELETE FROM VTECaction ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteVTECaction() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateVTECactionByRecord (const VTECaction * newStructPtr, const VTECaction * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetVTECactionPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateVTECaction(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateVTECaction(const VTECaction * structPtr)
{
    UpdateVTECactionByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertVTECaction(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueVTECaction(const VTECaction * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (VTECactionExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertVTECaction(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteVTECactionByRecord(const VTECaction * structPtr)
{
    char whereClause[BUFSIZ];
    GetVTECactionPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteVTECaction(whereClause));
}

/* ------------------------------------------------------------------------- */

bool VTECactionExists(const VTECaction * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetVTECactionPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectVTECactionCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetVTECactionPrimaryKeyWhereString (const VTECaction * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE action = '%s' ",
            structPtr->action);
}

/* ------------------------------------------------------------------------- */

void FreeVTECaction( VTECaction * structPtr)
{
    VTECaction* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( VTECaction * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetVTECactionDbStatus()
{
    return &dbStatus;
}

