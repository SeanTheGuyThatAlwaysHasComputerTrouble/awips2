/* ------------------------------------------------------------------------- */

int UpdateCountynumByRecord (const Countynum * newStructPtr, const Countynum * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetCountynumPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateCountynum(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateCountynum(const Countynum * structPtr)
{
    UpdateCountynumByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertCountynum(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueCountynum(const Countynum * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (CountynumExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertCountynum(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteCountynumByRecord(const Countynum * structPtr)
{
    char whereClause[BUFSIZ];
    GetCountynumPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteCountynum(whereClause));
}

/* ------------------------------------------------------------------------- */

bool CountynumExists(const Countynum * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetCountynumPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectCountynumCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetCountynumPrimaryKeyWhereString (const Countynum * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE lid = '%s'  AND state = '%s'  AND county = '%s' ",
            structPtr->lid, structPtr->state, structPtr->county);
}

/* ------------------------------------------------------------------------- */

void FreeCountynum( Countynum * structPtr)
{
    Countynum* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( Countynum * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

