/* ------------------------------------------------------------------------- */

void FreeLocTransmit( LocTransmit * structPtr)
{
    LocTransmit* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( LocTransmit * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

