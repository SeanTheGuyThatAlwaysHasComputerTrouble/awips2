int DeleteShefEx(const char *where)
{
    char deleteStatement[] = "DELETE FROM ShefEx ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteShefEx() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateShefExByRecord (const ShefEx * newStructPtr, const ShefEx * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetShefExPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateShefEx(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateShefEx(const ShefEx * structPtr)
{
    UpdateShefExByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertShefEx(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueShefEx(const ShefEx * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (ShefExExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertShefEx(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteShefExByRecord(const ShefEx * structPtr)
{
    char whereClause[BUFSIZ];
    GetShefExPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteShefEx(whereClause));
}

/* ------------------------------------------------------------------------- */

bool ShefExExists(const ShefEx * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetShefExPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectShefExCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetShefExPrimaryKeyWhereString (const ShefEx * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE extremum = '%s' ",
            structPtr->extremum);
}

/* ------------------------------------------------------------------------- */

void FreeShefEx( ShefEx * structPtr)
{
    ShefEx* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( ShefEx * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

