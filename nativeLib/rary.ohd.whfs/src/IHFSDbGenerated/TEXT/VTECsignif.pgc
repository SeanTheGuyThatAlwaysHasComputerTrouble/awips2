/* ------------------------------------------------------------------------- */

int UpdateVTECsignifByRecord (const VTECsignif * newStructPtr, const VTECsignif * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetVTECsignifPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateVTECsignif(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateVTECsignif(const VTECsignif * structPtr)
{
    UpdateVTECsignifByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertVTECsignif(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueVTECsignif(const VTECsignif * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (VTECsignifExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertVTECsignif(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteVTECsignifByRecord(const VTECsignif * structPtr)
{
    char whereClause[BUFSIZ];
    GetVTECsignifPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteVTECsignif(whereClause));
}

/* ------------------------------------------------------------------------- */

bool VTECsignifExists(const VTECsignif * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetVTECsignifPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectVTECsignifCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetVTECsignifPrimaryKeyWhereString (const VTECsignif * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE signif = '%s' ",
            structPtr->signif);
}

/* ------------------------------------------------------------------------- */

void FreeVTECsignif( VTECsignif * structPtr)
{
    VTECsignif* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( VTECsignif * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

