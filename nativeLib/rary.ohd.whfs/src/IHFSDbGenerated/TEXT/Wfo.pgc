int DeleteWfo(const char *where)
{
    char deleteStatement[] = "DELETE FROM Wfo ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteWfo() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateWfoByRecord (const Wfo * newStructPtr, const Wfo * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetWfoPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateWfo(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateWfo(const Wfo * structPtr)
{
    UpdateWfoByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertWfo(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueWfo(const Wfo * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (WfoExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertWfo(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteWfoByRecord(const Wfo * structPtr)
{
    char whereClause[BUFSIZ];
    GetWfoPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteWfo(whereClause));
}

/* ------------------------------------------------------------------------- */

bool WfoExists(const Wfo * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetWfoPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectWfoCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetWfoPrimaryKeyWhereString (const Wfo * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE wfo = '%s' ",
            structPtr->wfo);
}

/* ------------------------------------------------------------------------- */

void FreeWfo( Wfo * structPtr)
{
    Wfo* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( Wfo * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

