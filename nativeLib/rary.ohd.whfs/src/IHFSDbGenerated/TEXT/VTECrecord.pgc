            fprintf(stderr, "DeleteVTECrecord() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateVTECrecordByRecord (const VTECrecord * newStructPtr, const VTECrecord * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetVTECrecordPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateVTECrecord(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateVTECrecord(const VTECrecord * structPtr)
{
    UpdateVTECrecordByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertVTECrecord(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueVTECrecord(const VTECrecord * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (VTECrecordExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertVTECrecord(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteVTECrecordByRecord(const VTECrecord * structPtr)
{
    char whereClause[BUFSIZ];
    GetVTECrecordPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteVTECrecord(whereClause));
}

/* ------------------------------------------------------------------------- */

bool VTECrecordExists(const VTECrecord * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetVTECrecordPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectVTECrecordCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetVTECrecordPrimaryKeyWhereString (const VTECrecord * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE record = '%s' ",
            structPtr->record);
}

/* ------------------------------------------------------------------------- */

void FreeVTECrecord( VTECrecord * structPtr)
{
    VTECrecord* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( VTECrecord * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetVTECrecordDbStatus()
{
    return &dbStatus;
}

/* ------------------------------------------------------------------------- */

void SetVTECrecordErrorLogging(int value)
{
    errorLoggingOn = value;
    return;
}


