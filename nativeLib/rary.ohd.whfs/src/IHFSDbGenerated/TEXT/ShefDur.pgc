/* ------------------------------------------------------------------------- */

int UpdateShefDurByRecord (const ShefDur * newStructPtr, const ShefDur * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetShefDurPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateShefDur(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateShefDur(const ShefDur * structPtr)
{
    UpdateShefDurByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertShefDur(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueShefDur(const ShefDur * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (ShefDurExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertShefDur(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteShefDurByRecord(const ShefDur * structPtr)
{
    char whereClause[BUFSIZ];
    GetShefDurPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteShefDur(whereClause));
}

/* ------------------------------------------------------------------------- */

bool ShefDurExists(const ShefDur * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetShefDurPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectShefDurCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetShefDurPrimaryKeyWhereString (const ShefDur * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE dur = %d ",
            structPtr->dur);
}

/* ------------------------------------------------------------------------- */

void FreeShefDur( ShefDur * structPtr)
{
    ShefDur* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( ShefDur * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

