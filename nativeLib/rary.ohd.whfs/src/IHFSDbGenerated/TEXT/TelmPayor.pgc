/* ------------------------------------------------------------------------- */

int UpdateTelmPayorByRecord (const TelmPayor * newStructPtr, const TelmPayor * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetTelmPayorPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateTelmPayor(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateTelmPayor(const TelmPayor * structPtr)
{
    UpdateTelmPayorByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertTelmPayor(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueTelmPayor(const TelmPayor * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (TelmPayorExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertTelmPayor(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteTelmPayorByRecord(const TelmPayor * structPtr)
{
    char whereClause[BUFSIZ];
    GetTelmPayorPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteTelmPayor(whereClause));
}

/* ------------------------------------------------------------------------- */

bool TelmPayorExists(const TelmPayor * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetTelmPayorPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectTelmPayorCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetTelmPayorPrimaryKeyWhereString (const TelmPayor * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE payor = '%s' ",
            structPtr->payor);
}

/* ------------------------------------------------------------------------- */

void FreeTelmPayor( TelmPayor * structPtr)
{
    TelmPayor* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( TelmPayor * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

