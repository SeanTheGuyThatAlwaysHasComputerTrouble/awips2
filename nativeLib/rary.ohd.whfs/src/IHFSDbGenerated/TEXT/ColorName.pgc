/* ------------------------------------------------------------------------- */

int UpdateColorNameByRecord (const ColorName * newStructPtr, const ColorName * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetColorNamePrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateColorName(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateColorName(const ColorName * structPtr)
{
    UpdateColorNameByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertColorName(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueColorName(const ColorName * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (ColorNameExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertColorName(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteColorNameByRecord(const ColorName * structPtr)
{
    char whereClause[BUFSIZ];
    GetColorNamePrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteColorName(whereClause));
}

/* ------------------------------------------------------------------------- */

bool ColorNameExists(const ColorName * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetColorNamePrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectColorNameCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetColorNamePrimaryKeyWhereString (const ColorName * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE color_name = '%s' ",
            structPtr->color_name);
}

/* ------------------------------------------------------------------------- */

void FreeColorName( ColorName * structPtr)
{
    ColorName* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( ColorName * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

