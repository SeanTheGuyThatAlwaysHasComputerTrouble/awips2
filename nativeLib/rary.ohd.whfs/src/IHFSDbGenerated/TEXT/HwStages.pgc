/* ------------------------------------------------------------------------- */

void FreeHwStages( HwStages * structPtr)
{
    HwStages* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( HwStages * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

