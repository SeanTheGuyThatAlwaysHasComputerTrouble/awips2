/* ------------------------------------------------------------------------- */

int UpdateSnowMethodByRecord (const SnowMethod * newStructPtr, const SnowMethod * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetSnowMethodPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateSnowMethod(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateSnowMethod(const SnowMethod * structPtr)
{
    UpdateSnowMethodByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertSnowMethod(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueSnowMethod(const SnowMethod * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (SnowMethodExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertSnowMethod(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteSnowMethodByRecord(const SnowMethod * structPtr)
{
    char whereClause[BUFSIZ];
    GetSnowMethodPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteSnowMethod(whereClause));
}

/* ------------------------------------------------------------------------- */

bool SnowMethodExists(const SnowMethod * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetSnowMethodPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectSnowMethodCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetSnowMethodPrimaryKeyWhereString (const SnowMethod * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE snow_method = '%s' ",
            structPtr->snow_method);
}

/* ------------------------------------------------------------------------- */

void FreeSnowMethod( SnowMethod * structPtr)
{
    SnowMethod* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( SnowMethod * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

