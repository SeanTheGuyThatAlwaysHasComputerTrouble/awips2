/* ------------------------------------------------------------------------- */

int UpdateRadarRespByRecord (const RadarResp * newStructPtr, const RadarResp * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetRadarRespPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateRadarResp(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateRadarResp(const RadarResp * structPtr)
{
    UpdateRadarRespByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertRadarResp(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueRadarResp(const RadarResp * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (RadarRespExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertRadarResp(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteRadarRespByRecord(const RadarResp * structPtr)
{
    char whereClause[BUFSIZ];
    GetRadarRespPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteRadarResp(whereClause));
}

/* ------------------------------------------------------------------------- */

bool RadarRespExists(const RadarResp * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetRadarRespPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectRadarRespCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetRadarRespPrimaryKeyWhereString (const RadarResp * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE radid = '%s' ",
            structPtr->radid);
}

/* ------------------------------------------------------------------------- */

void FreeRadarResp( RadarResp * structPtr)
{
    RadarResp* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( RadarResp * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

