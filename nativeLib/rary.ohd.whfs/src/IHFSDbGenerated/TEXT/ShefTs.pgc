int DeleteShefTs(const char *where)
{
    char deleteStatement[] = "DELETE FROM ShefTs ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteShefTs() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateShefTsByRecord (const ShefTs * newStructPtr, const ShefTs * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetShefTsPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateShefTs(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateShefTs(const ShefTs * structPtr)
{
    UpdateShefTsByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertShefTs(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueShefTs(const ShefTs * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (ShefTsExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertShefTs(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteShefTsByRecord(const ShefTs * structPtr)
{
    char whereClause[BUFSIZ];
    GetShefTsPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteShefTs(whereClause));
}

/* ------------------------------------------------------------------------- */

bool ShefTsExists(const ShefTs * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetShefTsPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectShefTsCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetShefTsPrimaryKeyWhereString (const ShefTs * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE ts = '%s' ",
            structPtr->ts);
}

/* ------------------------------------------------------------------------- */

void FreeShefTs( ShefTs * structPtr)
{
    ShefTs* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( ShefTs * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetShefTsDbStatus()
{
    return &dbStatus;
}

