            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdatePostProcessorByRecord (const PostProcessor * newStructPtr, const PostProcessor * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetPostProcessorPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdatePostProcessor(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdatePostProcessor(const PostProcessor * structPtr)
{
    UpdatePostProcessorByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertPostProcessor(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniquePostProcessor(const PostProcessor * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (PostProcessorExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertPostProcessor(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeletePostProcessorByRecord(const PostProcessor * structPtr)
{
    char whereClause[BUFSIZ];
    GetPostProcessorPrimaryKeyWhereString(structPtr, whereClause);
    return (DeletePostProcessor(whereClause));
}

/* ------------------------------------------------------------------------- */

bool PostProcessorExists(const PostProcessor * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetPostProcessorPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectPostProcessorCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetPostProcessorPrimaryKeyWhereString (const PostProcessor * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE post_processor = '%s' ",
            structPtr->post_processor);
}

/* ------------------------------------------------------------------------- */

void FreePostProcessor( PostProcessor * structPtr)
{
    PostProcessor* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( PostProcessor * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetPostProcessorDbStatus()
{
    return &dbStatus;
}

/* ------------------------------------------------------------------------- */

void SetPostProcessorErrorLogging(int value)
{
    errorLoggingOn = value;
    return;
}


