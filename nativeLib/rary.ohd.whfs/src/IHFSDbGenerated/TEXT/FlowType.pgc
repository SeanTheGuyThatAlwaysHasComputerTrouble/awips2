/* ------------------------------------------------------------------------- */

int UpdateFlowTypeByRecord (const FlowType * newStructPtr, const FlowType * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetFlowTypePrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateFlowType(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateFlowType(const FlowType * structPtr)
{
    UpdateFlowTypeByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertFlowType(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueFlowType(const FlowType * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (FlowTypeExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertFlowType(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteFlowTypeByRecord(const FlowType * structPtr)
{
    char whereClause[BUFSIZ];
    GetFlowTypePrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteFlowType(whereClause));
}

/* ------------------------------------------------------------------------- */

bool FlowTypeExists(const FlowType * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetFlowTypePrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectFlowTypeCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetFlowTypePrimaryKeyWhereString (const FlowType * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE flowtype = '%s' ",
            structPtr->flowtype);
}

/* ------------------------------------------------------------------------- */

void FreeFlowType( FlowType * structPtr)
{
    FlowType* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( FlowType * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetFlowTypeDbStatus()
{
    return &dbStatus;
}

/* ------------------------------------------------------------------------- */

void SetFlowTypeErrorLogging(int value)
{
    errorLoggingOn = value;
    return;
}


