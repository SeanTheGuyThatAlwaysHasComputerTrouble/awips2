/* ------------------------------------------------------------------------- */

int UpdateVTECphenomByRecord (const VTECphenom * newStructPtr, const VTECphenom * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetVTECphenomPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateVTECphenom(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateVTECphenom(const VTECphenom * structPtr)
{
    UpdateVTECphenomByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertVTECphenom(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueVTECphenom(const VTECphenom * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (VTECphenomExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertVTECphenom(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteVTECphenomByRecord(const VTECphenom * structPtr)
{
    char whereClause[BUFSIZ];
    GetVTECphenomPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteVTECphenom(whereClause));
}

/* ------------------------------------------------------------------------- */

bool VTECphenomExists(const VTECphenom * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetVTECphenomPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectVTECphenomCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetVTECphenomPrimaryKeyWhereString (const VTECphenom * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE phenom = '%s' ",
            structPtr->phenom);
}

/* ------------------------------------------------------------------------- */

void FreeVTECphenom( VTECphenom * structPtr)
{
    VTECphenom* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( VTECphenom * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

