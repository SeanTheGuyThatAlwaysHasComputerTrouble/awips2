/* ------------------------------------------------------------------------- */

int UpdateRoutingMethodByRecord (const RoutingMethod * newStructPtr, const RoutingMethod * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetRoutingMethodPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateRoutingMethod(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateRoutingMethod(const RoutingMethod * structPtr)
{
    UpdateRoutingMethodByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertRoutingMethod(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueRoutingMethod(const RoutingMethod * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (RoutingMethodExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertRoutingMethod(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteRoutingMethodByRecord(const RoutingMethod * structPtr)
{
    char whereClause[BUFSIZ];
    GetRoutingMethodPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteRoutingMethod(whereClause));
}

/* ------------------------------------------------------------------------- */

bool RoutingMethodExists(const RoutingMethod * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetRoutingMethodPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectRoutingMethodCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetRoutingMethodPrimaryKeyWhereString (const RoutingMethod * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE hydraul_method = '%s' ",
            structPtr->hydraul_method);
}

/* ------------------------------------------------------------------------- */

void FreeRoutingMethod( RoutingMethod * structPtr)
{
    RoutingMethod* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( RoutingMethod * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

