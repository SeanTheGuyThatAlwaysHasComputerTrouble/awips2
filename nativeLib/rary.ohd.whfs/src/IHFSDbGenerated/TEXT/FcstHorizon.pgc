/* ------------------------------------------------------------------------- */

int UpdateFcstHorizonByRecord (const FcstHorizon * newStructPtr, const FcstHorizon * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetFcstHorizonPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateFcstHorizon(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateFcstHorizon(const FcstHorizon * structPtr)
{
    UpdateFcstHorizonByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertFcstHorizon(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueFcstHorizon(const FcstHorizon * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (FcstHorizonExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertFcstHorizon(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteFcstHorizonByRecord(const FcstHorizon * structPtr)
{
    char whereClause[BUFSIZ];
    GetFcstHorizonPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteFcstHorizon(whereClause));
}

/* ------------------------------------------------------------------------- */

bool FcstHorizonExists(const FcstHorizon * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetFcstHorizonPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectFcstHorizonCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetFcstHorizonPrimaryKeyWhereString (const FcstHorizon * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE fcst_horizon = '%s' ",
            structPtr->fcst_horizon);
}

/* ------------------------------------------------------------------------- */

void FreeFcstHorizon( FcstHorizon * structPtr)
{
    FcstHorizon* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( FcstHorizon * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

