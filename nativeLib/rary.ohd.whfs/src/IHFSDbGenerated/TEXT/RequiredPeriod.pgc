/* ------------------------------------------------------------------------- */

int UpdateRequiredPeriodByRecord (const RequiredPeriod * newStructPtr, const RequiredPeriod * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetRequiredPeriodPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateRequiredPeriod(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateRequiredPeriod(const RequiredPeriod * structPtr)
{
    UpdateRequiredPeriodByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertRequiredPeriod(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueRequiredPeriod(const RequiredPeriod * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (RequiredPeriodExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertRequiredPeriod(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteRequiredPeriodByRecord(const RequiredPeriod * structPtr)
{
    char whereClause[BUFSIZ];
    GetRequiredPeriodPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteRequiredPeriod(whereClause));
}

/* ------------------------------------------------------------------------- */

bool RequiredPeriodExists(const RequiredPeriod * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetRequiredPeriodPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectRequiredPeriodCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetRequiredPeriodPrimaryKeyWhereString (const RequiredPeriod * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE period_req = '%s' ",
            structPtr->period_req);
}

/* ------------------------------------------------------------------------- */

void FreeRequiredPeriod( RequiredPeriod * structPtr)
{
    RequiredPeriod* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( RequiredPeriod * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

