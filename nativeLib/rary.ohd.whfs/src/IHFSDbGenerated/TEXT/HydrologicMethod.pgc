int UpdateHydrologicMethodByRecord (const HydrologicMethod * newStructPtr, const HydrologicMethod * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetHydrologicMethodPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateHydrologicMethod(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateHydrologicMethod(const HydrologicMethod * structPtr)
{
    UpdateHydrologicMethodByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertHydrologicMethod(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueHydrologicMethod(const HydrologicMethod * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (HydrologicMethodExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertHydrologicMethod(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteHydrologicMethodByRecord(const HydrologicMethod * structPtr)
{
    char whereClause[BUFSIZ];
    GetHydrologicMethodPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteHydrologicMethod(whereClause));
}

/* ------------------------------------------------------------------------- */

bool HydrologicMethodExists(const HydrologicMethod * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetHydrologicMethodPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectHydrologicMethodCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetHydrologicMethodPrimaryKeyWhereString (const HydrologicMethod * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE hydrol_method = '%s' ",
            structPtr->hydrol_method);
}

/* ------------------------------------------------------------------------- */

void FreeHydrologicMethod( HydrologicMethod * structPtr)
{
    HydrologicMethod* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( HydrologicMethod * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

