    }

    memset(&dbs, '\0', sizeof(dbs));

    strcpy(dbs.lid, structPtr->lid);

    strcpy(dbs.state, structPtr->state);

    strcpy(dbs.zonenum, structPtr->zonenum);


    EXEC SQL INSERT INTO Zonenum (lid,state,
        zonenum        )
