            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateZonenumByRecord (const Zonenum * newStructPtr, const Zonenum * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetZonenumPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateZonenum(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateZonenum(const Zonenum * structPtr)
{
    UpdateZonenumByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertZonenum(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueZonenum(const Zonenum * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (ZonenumExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertZonenum(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteZonenumByRecord(const Zonenum * structPtr)
{
    char whereClause[BUFSIZ];
    GetZonenumPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteZonenum(whereClause));
}

/* ------------------------------------------------------------------------- */

bool ZonenumExists(const Zonenum * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetZonenumPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectZonenumCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetZonenumPrimaryKeyWhereString (const Zonenum * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE lid = '%s'  AND state = '%s'  AND zonenum = '%s' ",
            structPtr->lid, structPtr->state, structPtr->zonenum);
}

/* ------------------------------------------------------------------------- */

void FreeZonenum( Zonenum * structPtr)
{
    Zonenum* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( Zonenum * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

