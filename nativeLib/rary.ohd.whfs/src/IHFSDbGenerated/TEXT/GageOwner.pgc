/* ------------------------------------------------------------------------- */

int UpdateGageOwnerByRecord (const GageOwner * newStructPtr, const GageOwner * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetGageOwnerPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateGageOwner(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateGageOwner(const GageOwner * structPtr)
{
    UpdateGageOwnerByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertGageOwner(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueGageOwner(const GageOwner * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (GageOwnerExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertGageOwner(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteGageOwnerByRecord(const GageOwner * structPtr)
{
    char whereClause[BUFSIZ];
    GetGageOwnerPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteGageOwner(whereClause));
}

/* ------------------------------------------------------------------------- */

bool GageOwnerExists(const GageOwner * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetGageOwnerPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectGageOwnerCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetGageOwnerPrimaryKeyWhereString (const GageOwner * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE owner = '%s' ",
            structPtr->owner);
}

/* ------------------------------------------------------------------------- */

void FreeGageOwner( GageOwner * structPtr)
{
    GageOwner* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( GageOwner * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

