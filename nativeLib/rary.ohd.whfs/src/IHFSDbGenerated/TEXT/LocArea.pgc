            fprintf(stderr, "DeleteLocArea() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateLocAreaByRecord (const LocArea * newStructPtr, const LocArea * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetLocAreaPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateLocArea(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateLocArea(const LocArea * structPtr)
{
    UpdateLocAreaByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertLocArea(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueLocArea(const LocArea * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (LocAreaExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertLocArea(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteLocAreaByRecord(const LocArea * structPtr)
{
    char whereClause[BUFSIZ];
    GetLocAreaPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteLocArea(whereClause));
}

/* ------------------------------------------------------------------------- */

bool LocAreaExists(const LocArea * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetLocAreaPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectLocAreaCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetLocAreaPrimaryKeyWhereString (const LocArea * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE lid = '%s' ",
            structPtr->lid);
}

/* ------------------------------------------------------------------------- */

void FreeLocArea( LocArea * structPtr)
{
    LocArea* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( LocArea * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

/* ------------------------------------------------------------------------- */

DbStatus * GetLocAreaDbStatus()
{
    return &dbStatus;
}

