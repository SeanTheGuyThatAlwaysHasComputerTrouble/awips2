/* ------------------------------------------------------------------------- */

int UpdateDamTypesByRecord (const DamTypes * newStructPtr, const DamTypes * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetDamTypesPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateDamTypes(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateDamTypes(const DamTypes * structPtr)
{
    UpdateDamTypesByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertDamTypes(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueDamTypes(const DamTypes * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (DamTypesExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertDamTypes(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteDamTypesByRecord(const DamTypes * structPtr)
{
    char whereClause[BUFSIZ];
    GetDamTypesPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteDamTypes(whereClause));
}

/* ------------------------------------------------------------------------- */

bool DamTypesExists(const DamTypes * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetDamTypesPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectDamTypesCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetDamTypesPrimaryKeyWhereString (const DamTypes * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE type = '%s' ",
            structPtr->type);
}

/* ------------------------------------------------------------------------- */

void FreeDamTypes( DamTypes * structPtr)
{
    DamTypes* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( DamTypes * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

