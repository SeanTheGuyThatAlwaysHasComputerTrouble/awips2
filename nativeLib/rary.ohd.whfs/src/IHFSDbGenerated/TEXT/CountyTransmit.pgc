/* ------------------------------------------------------------------------- */

int UpdateCountyTransmitByRecord (const CountyTransmit * newStructPtr, const CountyTransmit * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetCountyTransmitPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateCountyTransmit(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateCountyTransmit(const CountyTransmit * structPtr)
{
    UpdateCountyTransmitByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertCountyTransmit(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueCountyTransmit(const CountyTransmit * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (CountyTransmitExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertCountyTransmit(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteCountyTransmitByRecord(const CountyTransmit * structPtr)
{
    char whereClause[BUFSIZ];
    GetCountyTransmitPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteCountyTransmit(whereClause));
}

/* ------------------------------------------------------------------------- */

bool CountyTransmitExists(const CountyTransmit * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetCountyTransmitPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectCountyTransmitCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetCountyTransmitPrimaryKeyWhereString (const CountyTransmit * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE call_sign = '%s'  AND county = '%s'  AND state = '%s' ",
            structPtr->call_sign, structPtr->county, structPtr->state);
}

/* ------------------------------------------------------------------------- */

void FreeCountyTransmit( CountyTransmit * structPtr)
{
    CountyTransmit* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( CountyTransmit * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

