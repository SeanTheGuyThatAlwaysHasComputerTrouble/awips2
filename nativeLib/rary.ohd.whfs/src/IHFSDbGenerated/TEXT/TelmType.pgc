/* ------------------------------------------------------------------------- */

int UpdateTelmTypeByRecord (const TelmType * newStructPtr, const TelmType * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetTelmTypePrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateTelmType(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateTelmType(const TelmType * structPtr)
{
    UpdateTelmTypeByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertTelmType(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueTelmType(const TelmType * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (TelmTypeExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertTelmType(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteTelmTypeByRecord(const TelmType * structPtr)
{
    char whereClause[BUFSIZ];
    GetTelmTypePrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteTelmType(whereClause));
}

/* ------------------------------------------------------------------------- */

bool TelmTypeExists(const TelmType * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetTelmTypePrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectTelmTypeCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetTelmTypePrimaryKeyWhereString (const TelmType * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE type = '%s' ",
            structPtr->type);
}

/* ------------------------------------------------------------------------- */

void FreeTelmType( TelmType * structPtr)
{
    TelmType* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( TelmType * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

