int DeleteRescap(const char *where)
{
    char deleteStatement[] = "DELETE FROM Rescap ";
    EXEC SQL BEGIN DECLARE SECTION;

    char queryBuffer[QUERY_LEN];

    EXEC SQL END DECLARE SECTION;

    strcpy(queryBuffer, deleteStatement);
    if ( (where != NULL) && (*where != '\0'))
    {
        strcat(queryBuffer, where);
    }
    EXEC SQL EXECUTE IMMEDIATE :queryBuffer;
    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "DeleteRescap() in Execute Immediate section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return(SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

/* ------------------------------------------------------------------------- */

int UpdateRescapByRecord (const Rescap * newStructPtr, const Rescap * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetRescapPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateRescap(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateRescap(const Rescap * structPtr)
{
    UpdateRescapByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertRescap(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueRescap(const Rescap * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (RescapExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertRescap(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteRescapByRecord(const Rescap * structPtr)
{
    char whereClause[BUFSIZ];
    GetRescapPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteRescap(whereClause));
}

/* ------------------------------------------------------------------------- */

bool RescapExists(const Rescap * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetRescapPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectRescapCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetRescapPrimaryKeyWhereString (const Rescap * structPtr, char returnWhereString[] )
{
    sprintf(returnWhereString, "WHERE lid = '%s'  AND elev = %lf ",
            structPtr->lid, structPtr->elev);
}

/* ------------------------------------------------------------------------- */

void FreeRescap( Rescap * structPtr)
{
    Rescap* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( Rescap * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

