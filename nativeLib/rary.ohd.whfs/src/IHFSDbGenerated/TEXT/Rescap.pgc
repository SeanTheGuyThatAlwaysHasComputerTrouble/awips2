int PutRescap(const Rescap * structPtr)
{
    EXEC SQL BEGIN DECLARE SECTION;

    struct Rescap_t  dbs;

    EXEC SQL END DECLARE SECTION;

    setDbStatusSqlCommand(&dbStatus, INSERT);

    if (structPtr == NULL)
    {
        return(ERR_BAD_ARGS);
    }

    memset(&dbs, '\0', sizeof(dbs));

    strcpy(dbs.lid, structPtr->lid);

    dbs.elev = structPtr->elev;

    dbs.storage = structPtr->storage;
    dbs.ind_storage = getIndicator(DOUBLE, (void *)&structPtr->storage);


    EXEC SQL INSERT INTO Rescap (lid,elev,
        storage        )
        VALUES (
         :dbs.lid, :dbs.elev, :dbs.storage:dbs.ind_storage);

    if (SQLCODE < 0)
    {
        if (errorLoggingOn)
        {
            fprintf(stderr, "PutRescap() in Insert section --- ERROR\n");
            fprintf(stderr, "SQLCODE = (%ld) sql state = (%s)\n", SQLCODE, sqlca.sqlstate);
            fprintf(stderr, "Error Message (%s)\n", sqlca.sqlerrm.sqlerrmc);
            fflush(stderr);
        }
        initDbStatus(&dbStatus);
        return (SQLCODE);
    }

    initDbStatus(&dbStatus);
    return(ERR_OK);
}

