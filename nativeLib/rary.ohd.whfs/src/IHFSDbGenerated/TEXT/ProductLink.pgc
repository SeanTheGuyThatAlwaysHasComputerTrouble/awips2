/* ------------------------------------------------------------------------- */

int UpdateProductLinkByRecord (const ProductLink * newStructPtr, const ProductLink * oldStructPtr)
{
    char whereClause[BUFSIZ];
    GetProductLinkPrimaryKeyWhereString(oldStructPtr, whereClause);
    return (UpdateProductLink(newStructPtr, whereClause));
}

/* ------------------------------------------------------------------------- */

int InsertOrUpdateProductLink(const ProductLink * structPtr)
{
    UpdateProductLinkByRecord(structPtr, structPtr);
    setDbStatusSqlCommand(&dbStatus, UPDATE);

    if ( (SQLCODE < 0) || (SQLCODE == 100) )
    {
        InsertProductLink(structPtr);
        setDbStatusSqlCommand(&dbStatus, INSERT);
    }

    initDbStatus(&dbStatus);
    return(SQLCODE);
}

/* ------------------------------------------------------------------------- */

int InsertIfUniqueProductLink(const ProductLink * structPtr, bool *isUnique)
{
    int resultCode = 0;
    if (ProductLinkExists(structPtr))
    {
        setDbStatusSqlCommand(&dbStatus, SELECT);
        *isUnique = false;
        resultCode = dbStatus.sql_code;
    }
    else
    {
        resultCode = dbStatus.sql_code;
        if (resultCode == 0)
        {
            InsertProductLink(structPtr);
            setDbStatusSqlCommand(&dbStatus, INSERT);
            *isUnique = true;
            resultCode = dbStatus.sql_code;
        }
        else
        {
            *isUnique = false;
        }
    }
    initDbStatus(&dbStatus);

    return(resultCode);
}

/* ------------------------------------------------------------------------- */

int DeleteProductLinkByRecord(const ProductLink * structPtr)
{
    char whereClause[BUFSIZ];
    GetProductLinkPrimaryKeyWhereString(structPtr, whereClause);
    return (DeleteProductLink(whereClause));
}

/* ------------------------------------------------------------------------- */

bool ProductLinkExists(const ProductLink * structPtr)
{
    int result = false;
    int rowCount = 0;
    char whereString[QUERY_LEN];

    GetProductLinkPrimaryKeyWhereString(structPtr, whereString);
    rowCount = SelectProductLinkCount(whereString);

    if (rowCount > 0)
    {
        result = true;
    }
    else
    {
        result = false;
    }

    return(result);
}

/* ------------------------------------------------------------------------- */

void GetProductLinkPrimaryKeyWhereString (const ProductLink * structPtr, char returnWhereString[] )
{
    char time_buffer0[40];
    char time_buffer1[40];
    sprintf(returnWhereString, "WHERE lid = '%s'  AND product_id = '%s'  AND producttime = '%s'  AND postingtime = '%s' ",
            structPtr->lid, structPtr->product_id, dtimet_to_ansi(structPtr->producttime, time_buffer0), dtimet_to_ansi(structPtr->postingtime, time_buffer1));
}

/* ------------------------------------------------------------------------- */

void FreeProductLink( ProductLink * structPtr)
{
    ProductLink* nextPtr = NULL;

    while (structPtr != NULL)
    {
        nextPtr = ( ProductLink * ) ListNext ( &structPtr->node );
        free (structPtr);
        structPtr = nextPtr;
    }
    return;
}

