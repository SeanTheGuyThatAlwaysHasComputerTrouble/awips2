OBJ = /fs/hseb/ob83/ohd/pproc/obj/post_analysis/TEXT

CC       = /usr/bin/gcc
COPTIM   =
CFLAGS   = $(COPTIM) -fno-strict-aliasing -DUNIX -DGRTYPE_PS -DGRTYPE_XWIN -DLINX -DX1D -DTIMER -D__USE_GNU -DUSE_XM_MWMUTIL_H -DUSE_XGPM -Wall -Wno-unused-variable -Wno-comment

INSERTS      = -I/fs/hseb/ob83/ohd/whfs_lib/inc/general  \
               -I/fs/hseb/ob83/ohd/whfs_lib/inc/dbms  \
               -I/fs/hseb/ob83/ohd/whfs_lib/inc/gui  \
               -I/fs/hseb/ob83/ohd/ffg/inc  \
               -I/usr/local/netcdf/include  \
               -I/fs/hseb/ob83/ohd/pproc_lib/inc  \
               -I/fs/hseb/ob83/ohd/pproc/inc  \
               -I/fs/hseb/ob83/ohd/ifp/inc  \
               -I/fs/hseb/ob83/ohd/util/inc  \
               -I/fs/hseb/ob83/ohd/util/inc/messenger_inc

INSERTS_CC   = -I/usr/include/informix/esql/  \
               -I/usr/X11R6/include  \
               -I/usr/include  \
               -I/usr/lib/gcc/i386-redhat-linux/3.4.6/include  \
               -I/usr/local/netcdf/include  \
               -I/usr/local/jdk-1.5.0_04/include  \
               -I/usr/local/jdk-1.5.0_04/include/linux  \
               -I/usr/include  \
               -I../TEXT

.SUFFIXES:
.SUFFIXES:   .c .h .o

.c.o:
	@echo "-------------------------------- $<"
	@$(CC) $(CFLAGS) $(INSERTS_CC) $(INSERTS) -c $< \
	   && chmod 664 ${*F}.o  \
	   && echo "o - ${*F}.o" \
	   || echo "e - ${*F}"
	@echo "--------------------------------"

objects:  \
     $(OBJ)/close_gage.o  \
     $(OBJ)/copy_area.o  \
     $(OBJ)/fill_pixmap.o  \
     $(OBJ)/main_post.o  \
     $(OBJ)/merge.o  \
     $(OBJ)/post_add_overlays.o  \
     $(OBJ)/post_callbacks.o  \
     $(OBJ)/post_choose_date.o  \
     $(OBJ)/post_dates.o  \
     $(OBJ)/post_gage_display.o  \
     $(OBJ)/post_gage_table.o  \
     $(OBJ)/post_locate.o  \
     $(OBJ)/post_table_data.o  \
     $(OBJ)/postX.o  \
     $(OBJ)/read_geo_data.o  \
     $(OBJ)/read_papref.o  \
     $(OBJ)/read_postparam.o  \
     $(OBJ)/set_coloroverlays.o  \
     $(OBJ)/set_colorvalues.o  \
     $(OBJ)/show_overlays.o  \
     $(OBJ)/st3post.o  \
     $(OBJ)/xor.o

$(OBJ)/close_gage.o: close_gage.c
	@make -k -f ./Makefile_obj_lx ${@F}
	@-[ -f ${@F} ] && mv ${@F} $(OBJ) || :

