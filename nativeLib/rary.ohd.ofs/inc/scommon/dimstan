C                                           *--> FROM SCOMMON.DIMSTAN
C  ARRAYS FOR STAN PARAMETERS
      CHARACTER*8 STAID,TSTAID
      CHARACTER*20 DESCRP,TDESCRP
      DIMENSION STALOC(2),STALOCO(2),STACOR(2)
      PARAMETER (MGPS=4)
      DIMENSION GPS(MGPS),IPARM(MGPS)
      CHARACTER*4 GPS
      PARAMETER (MSRCCD=5,MSRCID=2)
      DIMENSION SRCCD(MSRCCD),SRCID(MSRCID,MSRCCD)
      CHARACTER*4 SRCCD,SRCID
      PARAMETER (MGOESN=10)
      DIMENSION GOESN(MGOESN)
      PARAMETER (MCDASN=10)
      DIMENSION CDASN(MCDASN)
