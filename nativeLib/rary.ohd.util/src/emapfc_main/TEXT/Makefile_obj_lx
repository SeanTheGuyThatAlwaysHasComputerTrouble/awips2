OBJ = /fs/hseb/ob83/ohd/util/obj/emapfc_main/TEXT

CC       = /usr/bin/gcc
COPTIM   =
CFLAGS   = $(COPTIM) -fno-strict-aliasing -DUNIX -DGRTYPE_PS -DGRTYPE_XWIN -DLINX -DX1D -DTIMER -D__USE_GNU -DUSE_XM_MWMUTIL_H -DUSE_XGPM -Wall -Wno-unused-variable -Wno-comment

INSERTS      = -I/fs/hseb/ob83/ohd/util/inc  \
               -I/fs/hseb/ob83/ohd/ofs/inc

INSERTS_CC   = -I/usr/include/informix/esql/  \
               -I/usr/X11R6/include  \
               -I/usr/include  \
               -I/usr/lib/gcc/i386-redhat-linux/3.4.6/include  \
               -I/usr/local/netcdf/include  \
               -I/usr/local/jdk-1.5.0_04/include  \
               -I/usr/local/jdk-1.5.0_04/include/linux  \
               -I/usr/include  \
               -I../TEXT

.SUFFIXES:
.SUFFIXES:   .c .h .o

.c.o:
	@echo "-------------------------------- $<"
	@$(CC) $(CFLAGS) $(INSERTS_CC) $(INSERTS) -c $< \
	   && chmod 664 ${*F}.o  \
	   && echo "o - ${*F}.o" \
	   || echo "e - ${*F}"
	@echo "--------------------------------"

objects:  \
     $(OBJ)/geoidtst.o  \
     $(OBJ)/testemapf.o  \
     $(OBJ)/tseqlat.o

$(OBJ)/geoidtst.o: geoidtst.c
	@make -k -f ./Makefile_obj_lx ${@F}
	@-[ -f ${@F} ] && mv ${@F} $(OBJ) || :

$(OBJ)/testemapf.o: testemapf.c
	@make -k -f ./Makefile_obj_lx ${@F}
	@-[ -f ${@F} ] && mv ${@F} $(OBJ) || :

$(OBJ)/tseqlat.o: tseqlat.c
	@make -k -f ./Makefile_obj_lx ${@F}
	@-[ -f ${@F} ] && mv ${@F} $(OBJ) || :


