<pluginNotificationList>
    <pluginNotification>
        <endpointName>gfe.spcWatch</endpointName>
        <format>PDO</format>
        <endpointType>VM</endpointType>
        <metadataMap>
            <mapping key="pluginName">
                <constraint constraintValue="warning,practicewarning" constraintType="IN"/>
            </mapping>
            <mapping key="pil">
                <constraint constraintValue="WOU" constraintType="EQUALS"/>
            </mapping>
        </metadataMap>
    </pluginNotification>
    <pluginNotification>
        <endpointName>gfe.tpcWatch</endpointName>
        <format>PDO</format>
        <endpointType>VM</endpointType>
        <metadataMap>
            <mapping key="pluginName">
                <constraint constraintValue="warning,practicewarning" constraintType="IN"/>
            </mapping>
            <mapping key="pil">
                <constraint constraintValue="TCV,HLS" constraintType="IN"/>
            </mapping>
        </metadataMap>
    </pluginNotification>
</pluginNotificationList>
