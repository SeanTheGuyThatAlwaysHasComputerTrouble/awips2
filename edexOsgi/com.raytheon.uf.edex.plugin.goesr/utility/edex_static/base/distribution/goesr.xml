    <!--  IT_ABI-L2-CMIPF-M3C05_G16_s2005155201500_e2005155203700_c2014058132255.nc -->
    <regex>^IT_ABI-L2.*</regex>
    <regex>^OR_ABI-L2-\w{3,5}(C|F|M1|M2)-M[34]_G\d\d_s\d{14}_e\d{14}_c\d{14}.nc$</regex>
</requestPatterns>
