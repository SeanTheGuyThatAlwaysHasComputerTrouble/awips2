<styleRuleset>
	<!-- GOES-R Derived Satellite Rules -->
	<styleRule>
		<paramLevelMatches>
			<parameter>satMoisture</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits label="C">K</displayUnits>
			<range scale="LINEAR">
				<minValue>-1.0</minValue>
				<maxValue>16.0</maxValue>
			</range>
			<defaultColormap>Grid/gridded data</defaultColormap>
			<colorbarLabeling>
				<values>0 3 6 9 12 15</values>
			</colorbarLabeling>
		</imageStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>satCloudPhase</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits label="C">K</displayUnits>
			<range scale="LINEAR">
				<minValue>-8.0</minValue>
				<maxValue>24.0</maxValue>
			</range>
			<defaultColormap>Grid/gridded data</defaultColormap>
			<colorbarLabeling>
				<values>-5 0 5 10 15 20</values>
			</colorbarLabeling>
		</imageStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>satFog</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits label="C">K</displayUnits>
			<range scale="LINEAR">
				<minValue>-10.0</minValue>
				<maxValue>100.0</maxValue>
			</range>
			<defaultColormap>Grid/gridded data</defaultColormap>
			<colorbarLabeling>
				<values>0 20 40 60 80 100</values>
			</colorbarLabeling>
		</imageStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>satUpperLevelInfo</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits label="C">K</displayUnits>
			<range scale="LINEAR">
				<minValue>-30.0</minValue>
				<maxValue>90.0</maxValue>
			</range>
			<defaultColormap>Grid/gridded data</defaultColormap>
			<colorbarLabeling>
				<values>-20 0 20 40 60 80</values>
			</colorbarLabeling>
		</imageStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>satSnow</parameter>
			<parameter>satVegetation</parameter>
		</paramLevelMatches>
		<imageStyle>
			<range scale="LINEAR">
				<minValue>-1300.0</minValue>
				<maxValue>300.0</maxValue>
			</range>
			<defaultColormap>Grid/gridded data</defaultColormap>
		</imageStyle>
	</styleRule>
</styleRuleset>